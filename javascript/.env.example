# NodeMaven API Configuration
# Copy this file to .env and fill in your actual values

# Required: Your NodeMaven API key
NODEMAVEN_APIKEY=your_api_key_here

# Optional: API base URL (default: https://dashboard.nodemaven.com)
NODEMAVEN_BASE_URL=https://dashboard.nodemaven.com

# Optional: Proxy host (default: gate.nodemaven.com)
NODEMAVEN_PROXY_HOST=gate.nodemaven.com

# Optional: HTTP proxy port (default: 8080)
NODEMAVEN_HTTP_PORT=8080

# Optional: SOCKS5 proxy port (default: 1080)
NODEMAVEN_SOCKS5_PORT=1080

# Optional: Request timeout in milliseconds (default: 30000)
REQUEST_TIMEOUT=30000 