# NodeMaven PHP SDK Configuration
# Copy this file to .env and fill in your actual values

# Your NodeMaven API key (required)
# Get your API key from: https://dashboard.nodemaven.com
NODEMAVEN_APIKEY=your_api_key_here

# API Configuration (optional - uses defaults if not specified)
NODEMAVEN_BASE_URL=https://dashboard.nodemaven.com
NODEMAVEN_PROXY_HOST=gate.nodemaven.com
NODEMAVEN_HTTP_PORT=8080
NODEMAVEN_SOCKS5_PORT=1080

# Request Configuration (optional)
REQUEST_TIMEOUT=30
DEBUG=false

# Development / Testing
LOG_LEVEL=info 